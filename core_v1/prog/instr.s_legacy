.text
.global main
main:
addi a2, zero, 0x41
addi a2, zero, 0x41
addi a2, zero, 0x41
addi a2, zero, 0x41
addi a2, zero, 0x41
jal t5, putc
addi a2, zero, 0x42
jal t5, putc
addi a2, zero, 0x43
jal t5, putc
jal t5, 0

putc:
    addi t1, zero, 0x01
loop:
    lw t2, 0x408(zero)
    beq t1, t2, loop
    sw a2, 0x404(zero)
    jalr t5, 0(t5)
